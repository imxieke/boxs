# Frontend Package Manager
alias fmi="pnpm i"
alias fmig="pnpm i -g"
alias fmag="pnpm add -g"
alias fma="pnpm add --save"
alias fmad="pnpm add --save-dev"

alias fml="pnpm -g list | awk -F '\n' '{print ${1}}'"
alias fmo="pnpm -g outdated"
alias fmuu="pnpm -g update"
alias fmr="pnpm rm -g"
alias fmrd="npm run dev"